class Student{
    String name;
    int marks;
    
    void display(){
        System.out.println("Name:"+name);
        System.out.println("Marks:"+marks);
    }
    
    public static void main(String[]args){
        Student S1=new Student();
        S1.name="Soumya";
        S1.marks=95;
        
        S1.display();
    }
}
