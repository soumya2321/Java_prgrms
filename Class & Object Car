class Car {
    String brand;
    String model;
    int price;

    public static void main(String[] args) {
        Car c1 = new Car();

        c1.brand = "Toyota";
        c1.model = "Innova";
        c1.price = 2000000;

        System.out.println(c1.brand);
        System.out.println(c1.model);
        System.out.println(c1.price);
    }
}
